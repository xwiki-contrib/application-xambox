<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
   <groupId>org.xwiki.commons</groupId>
   <artifactId>xwiki-commons-pom</artifactId>
   <version>4.5.4</version>
  </parent>
  <groupId>org.xwiki.contrib</groupId>
  <artifactId>application-xambox-parent</artifactId>
  <packaging>pom</packaging>
  <name>File Manager Xambox Connector Parent</name>
  <version>1.0.2-SNAPSHOT</version>
  <description>
    Extension to the File Manager Application allowing to connect to the Xambox Anywhere service (http://www.xambox.com). It allows to import files from the Xambox Service into the File Manager application and link File Manager entries to other wiki pages using the File Manager Macro.
  </description>
  <developers>
    <developer>
      <id>ldubost</id>
      <name>Ludovic Dubost</name>
    </developer>
  </developers>
  <modules>
    <module>api</module>
    <module>wiki</module>
  </modules>
  <scm>
   <connection>scm:git:git://github.com/xwiki-contrib/application-xambox.git</connection>
   <developerConnection>scm:git:git@github.com:xwiki-contrib/application-xambox.git</developerConnection>
   <url>https://github.com/xwiki-contrib/application-xambox</url>
   <tag>HEAD</tag>
  </scm>
 <distributionManagement>
   <repository>
     <id>xwiki-staging</id>
     <name>XWiki Staging Repository</name>
     <url>http://nexus.xwiki.org/nexus/service/local/staging/deploy/maven2/</url>
   </repository>
 </distributionManagement>
</project>
